mixin sub-category()
  ul.sub-category.grid-tpl-12&attributes(attributes)
    each item in catalogMenulist
      if (item.text == 'Акции' || item.text == 'Новинки' || item.text == 'Свадьба')
      else
        li.sub-category__item.grid-tpl-col-6.grid-tpl-col-md-3
          +sub-category-link({
            text: item.text,
            href: item.href,
          })(class=`sub-category-link--${item.icon}`)
